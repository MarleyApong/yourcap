{
  "expo": {
    "name": "yourcap",
    "slug": "yourcap-slug",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo/logo.png",
    "scheme": "yourcap",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo/logo.png",
        "backgroundColor": "#562d8f"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.yourcap"
    },
    "notification": {
      "icon": "./assets/images/logo/logo-white.png",
      "color": "#562d8f",
      "androidMode": "default",
      "androidCollapsedTitle": "#{unread_notifications} nouvelle(s) notification(s)"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo/logo.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://yourcap-slug.com"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo/logo-white.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#562d8f"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo/logo-white.png",
          "color": "#562d8f",
          "defaultChannel": "default"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "db21fc39-6fb2-4b12-b025-f82890ba36ce"
      }
    }
  }
}
